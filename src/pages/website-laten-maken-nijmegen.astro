---
import Layout from '~/layouts/PageLayout.astro';
import Prices3 from '~/components/widgets/Pricing3.astro';
import FAQs from '~/components/widgets/FAQs.astro';
import Steps from '~/components/widgets/Steps.astro';
import Features3 from '~/components/widgets/Features3.astro';
import CallToAction from '~/components/widgets/CallToAction.astro';
import BlogLatestPosts from '~/components/widgets/BlogLatestPosts.astro';
import Testimonials from '~/components/widgets/Testimonials.astro';
import Hero from '~/components/widgets/Hero.astro';

const images = [
    { url: "https://www.allooi.nl", imgSrc: "/_astro/allooi.C0F7zWvX.png", alt: "boekhoudkantoor website laten maken Nijmegen" },
    { url: "https://tppbaak.nl", imgSrc: "/_astro/tppbaak.DpD3ExtC.png", alt: "Tandpotheticus website TPPBaak (regio Nijmegen)" },
    { url: "https://www.zvvzittert.nl", imgSrc: "/_astro/ZVV Zittert.BzWReYwv.png", alt: "website gemaakt zaalvoetbal regio Nijmegen" },
    { url: "https://weggeefdeals.nl/", imgSrc: "/_astro/weggeefdeals.DZWuE7R0.png", alt: "Website weggeefdeals Nijmegen" }
  ];

const metadata = {
  title: 'Magick Media - Website laten maken Nijmagen',
  description:'Voor een professionele website in de regio Nijmegen ben je bij ons aan het juiste adres. Wij maken jouw online visie werkelijkheid.',
  ignoreTitleTemplate: true,
};
---

<Layout metadata={metadata}>

  <!-- Hero Widget ******************* -->
  <Hero
    tagline="Website laten maken Nijmegen"
    actions={[
      {
        variant: 'primary',
        text: 'Plan een gesprek',
        href: '/plan-een-gesprek/',
      },
      { text: 'Tarieven', href: '#tarieven' },
    ]}
  >
    <Fragment slot="title">
      Jouw bedrijf online met een voordelige website? <span class='text-accent dark:text-white highlight'>Magick Media</span> biedt betaalbare oplossingen in de regio Nijmegen!
    </Fragment>

    <Fragment slot="subtitle">
      <span class="sm:inline">
        Welkom bij Magick Media, waar je voor een scherpe prijs een professionele <b>website kunt laten maken in de regio Nijmegen</b> zonder compromissen.
        </span>
    </Fragment>
  </Hero>

  <!-- Features3 Widget ************** -->

  <Features3
    title="Onze garanties"
    subtitle="Laat je je website door ons maken? Dan kun je deze voordelen verwachten die zorgen voor een sterk online resultaat."
